!$warning_text = $white
!$danger_text = $white

!procedure $code($msg)
  <font:Consolas>$msg</font>
!endprocedure

!procedure $participant($sprite, $title)
    participant "<$$sprite,color=$light_orange>\n\n$title"
!endprocedure

!procedure $primary_scheme()
	FontColor $primary_text
	BorderColor $light_orange
	BackgroundColor $peach
!endprocedure

