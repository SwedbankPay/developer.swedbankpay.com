        AttributeFontColor $primary_text
        AttributeFontSize 11
    }
!endsub

!startsub queue
    skinparam queue {
        $primary_scheme()
    }
!endsub

!startsub card
    skinparam card {
        $primary_scheme()
    }
!endsub

